{
    "Statement": [
     {
      "Action": [
       "s3:GetBucketLocation"
      ],
      "Effect": "Allow",
      "Principal": {
       "AWS": [
        "*"
       ]
      },
      "Resource": [
       "arn:aws:s3:::mybucket"
      ]
     },
     {
      "Action": [
       "s3:GetObject"
      ],
      "Effect": "Allow",
      "Principal": {
       "AWS": [
        "*"
       ]
      },
      "Resource": [
       "arn:aws:s3:::mybucket/*"
      ]
     }
    ],
    "Version": "2012-10-17"
   }